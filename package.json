{
  "name": "asset-to-design",
  "version": "1.0.0",
  "description": "A Figma plugin for bulk importing images and automatically mapping them to named layers with intelligent matching algorithms",
  "main": "code.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "dev": "tsc && figma build",
    "clean": "rm -f code.js"
  },
  "keywords": [
    "figma",
    "plugin",
    "images",
    "import",
    "bulk-import",
    "design",
    "assets",
    "layer-mapping",
    "design-systems"
  ],
  "author": "Andre Lima",
  "license": "MIT",
  "devDependencies": {
    "@figma/plugin-typings": "^1.82.0",
    "typescript": "^5.0.0"
  },
  "figma": {
    "id": "copy-text-between-frames",
    "name": "Asset.to.Design"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Andrelimadesign/move.text.git"
  },
  "bugs": {
